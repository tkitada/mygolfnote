<header>
  <div>
    <%= link_to root_path do %>
      <%= image_tag "mygolfnote_logo.png", style: "width: 10%;" %>
    <% end %>
  </div>
  <nav class="navbar navbar-expand-lg navbar-light">
    <% if admin_signed_in? %>
      <%= link_to "サインアウト", destroy_admin_session_path, method: :delete %>
    <% elsif user_signed_in? %>
      <%= link_to "Golf note", public_practice_posts_new_path %>
      <%= link_to "サインアウト", destroy_user_session_path, method: :delete %>
    <% else %>
      <%= link_to "新規登録", new_user_registration_path %>
      <%= link_to "サインイン", new_user_session_path %>
      <%= link_to "about", about_path %>
    <% end %>
  </nav>

</header>